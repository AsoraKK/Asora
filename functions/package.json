{
  "name": "asora-backend-functions",
  "version": "1.0.0",
  "description": "Asora platform backend Azure Functions with AI-powered moderation",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rimraf dist && tsc && tsc-alias && node scripts/write-dist-entry.cjs",
    "test": "jest --config jest.config.ts --runInBand",
    "test:coverage": "jest --config jest.config.ts --runInBand --coverage",
    "verify:dist": "node scripts/verify-dist-structure.cjs ../functions-dist.zip"
  },
  "dependencies": {
    "@azure/app-configuration": "^1.6.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/functions": "^4.9.0",
    "@azure/identity": "^4.13.0",
    "@azure/keyvault-secrets": "^4.10.0",
    "@azure/storage-blob": "^12.29.1",
    "@azure/storage-queue": "^12.28.1",
    "applicationinsights": "^2.9.2",
    "archiver": "^7.0.1",
    "axios": "1.12.2",
    "fast-json-stable-stringify": "^2.1.0",
    "google-auth-library": "^9.15.1",
    "ioredis": "^5.3.0",
    "joi": "^17.12.1",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.7.2",
    "pg": "^8.16.3",
    "pino": "^8.12.0",
    "redis": "^4.6.0",
    "undici": "^5.29.0",
    "uuid": "10.0.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "jest": "^29.7.0",
    "nock": "^13.3.0",
    "rimraf": "^5.0.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^3.12.6",
    "typescript": "5.9.3"
  },
  "overrides": {
    "typescript": "5.5.4"
  },
  "keywords": [
    "azure-functions",
    "typescript",
    "cosmos-db",
    "jwt",
    "ai-moderation",
    "hive-ai",
    "asora"
  ],
  "author": "Asora Team",
  "license": "MIT"
}
